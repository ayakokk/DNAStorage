int **Fenc[][] =
  { {-1,-1,-1,-1}, //  0: 0000 (invalid)
    { 0, 0, 0, 0}, //  1: 0001
    { 1, 1, 1, 1}, //  2: 0010
    { 0, 0, 1, 1}, //  3: 0011
    { 2, 2, 2, 2}, //  4: 0100
    { 0, 0, 2, 2}, //  5: 0101
    { 1, 1, 2, 2}, //  6: 0110
    { 0, 1, 2, 2}, //  7: 0111
    { 3, 3, 3, 3}, //  8: 1000
    { 0, 0, 3, 3}, //  9: 1001
    { 1, 1, 3, 3}, // 10: 1010
    { 0, 1, 3, 3}, // 11: 1011
    { 2, 2, 3, 3}, // 12: 1100
    { 0, 0, 2, 3}, // 13: 1101
    { 1, 1, 2, 3}, // 14: 1110
    { 0, 1, 2, 3}  // 15: 1111
  }
  
