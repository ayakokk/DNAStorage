set format x "%.3f"
set format y "%.1f"
plot "rateA.dat" using 1:2 with linespoints lw 3 title "code 1",\
     "rateA.dat" using 1:3 with linespoints lw 3 title "code 2",\
     "rateA.dat" using 1:4 with linespoints lw 3 title "code 3"